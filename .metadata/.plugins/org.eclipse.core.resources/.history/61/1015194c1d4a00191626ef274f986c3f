#ifndef HTTPSERVER_H_
#define HTTPSERVER_H_

enum HTTP_METHOD
{
	eGET,
	ePOST,
	ePUT
};

class httpserver
{
private:
	(void*)(*mGetHandle)(void*);// calls the getfunction of the calling class, calling class needs to return the HTTP page


public:
	httpserver();
	virtual ~httpserver();
};

#endif /* HTTPSERVER_H_ */
