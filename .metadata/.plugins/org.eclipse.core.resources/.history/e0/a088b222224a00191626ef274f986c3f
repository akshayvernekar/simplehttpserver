void httpserver::addHandle(HTTP_METHOD method,(void*)(*handle)(void*))
{
	switch (method)
	{
	case eGET:
		mGetHandle = handle;
		break;
	case ePut:
		break;
	case ePost:
		break;
	case default:
		std::cout << "unsupported HTTP method" << std::endl;
	}
}


